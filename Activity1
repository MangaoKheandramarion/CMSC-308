using System;
namespace ConsoleApplication1
{
	class Program
	{
		public static void Main(string[] args)
		{
			string [] emails = {"kheandramangao@gmail.com", 
								"kheandramarionmangao@gmail.com", 
								"kheanmangao@gmail.com"};
			
			string [] username = {"kheandramangao", 
								 "kheandramarionmangao", 
								 "kheanmangao"};
			
			string [] passwords = {"kheandra",
				   				   "kheandramarion",
				   				   "khean"};
		
		Console.WriteLine("Index     : " + 0);
		Console.WriteLine("emails    : " + emails [0]);
		Console.WriteLine("usernames : " + username [0]);
		Console.WriteLine("passwords : " + passwords [0]);
		}
	}
}
